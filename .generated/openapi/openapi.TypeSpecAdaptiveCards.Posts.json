{
  "openapi": "3.0.0",
  "info": {
    "title": "Blog Posts",
    "x-ai-description": "Plugin to view posts objects.",
    "x-description-for-human": "Manage your blog posts.",
    "version": "0.0.0"
  },
  "tags": [],
  "paths": {
    "/posts": {
      "get": {
        "operationId": "listPosts",
        "description": "List all posts.",
        "parameters": [],
        "responses": {
          "200": {
            "description": "The request has succeeded.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Post"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Post": {
        "type": "object",
        "required": [
          "title"
        ],
        "properties": {
          "id": {
            "type": "string",
            "description": "The unique identifier for the post."
          },
          "userId": {
            "type": "string",
            "description": "The user who created the post."
          },
          "title": {
            "type": "string",
            "description": "The short summary or title of the post."
          },
          "body": {
            "type": "string",
            "description": "The detailed description of the post."
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "https://jsonplaceholder.typicode.com",
      "description": "Posts API",
      "variables": {}
    }
  ]
}
